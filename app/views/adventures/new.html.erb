<h1>Create New Adventure</h1>

<%= form_for @adventure do |f| %>
  <%= f.text_field :title, placeholder: "Title" %>
  <%= f.text_field :author, placeholder: "Author" %>
<% end %>

<%= link_to 'Show', @adventure %>
<%= link_to 'Back', adventures_path %>
<%= link_to 'Add Page', new_adventure_page_path(@adventure, @page) %>